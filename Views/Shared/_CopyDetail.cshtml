@model Asset_Oracle.Models.SV_SERVICEDETAILS
@{
    Layout = null;
}
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/Content/Site.css" rel="stylesheet" />
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/ProjectScript.js"></script>
<div class="container">

    @using (Html.BeginForm("_CopyDetail", "Service", FormMethod.Post))
    {
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="panel-title">Copy Records based On Release</div>
            </div>
            <div style="padding-top:10px" class="panel-body">
                <div style="display:none" id="login-alert" class="alert alert-danger col-sm-12"></div>
                <form id="loginform" class="form-horizontal" role="form">
                    <div class="form-group">
                        <label for="lastname" class="col-md-3 control-label">Release</label>
                        <div class="input-group col-md-9">
                            @Html.TextBoxFor((model => model.RELEASE), new { @class = "form-control", @placeholder = "Enter Release", @autofocus = true })
                            <p style="color: red">@TempData["ErrorResponse"]</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <!-- Button -->
                        <div class="col-md-offset-3 input-group col-md-9">
                            <button id="btnlogin" name="Login" type="submit" class="btn btn-info" ><i class="icon-hand-right"></i> &nbsp Submit</button>&nbsp; &nbsp; &nbsp;
                            <button id="btnCancel" name="Cancel" type="button" class="btn btn-info" onclick="windowClose();"><i class="icon-hand-right"></i> &nbsp Cancel</button>
                        </div>
                    </div>
                    <div class="form-group row ">
                        <!-- Button -->
                        <div class="col-md-offset-3 input-group col-md-9">
                            <b style="color:red;text-align:left;">@Html.Raw(TempData["msg"])</b>

                        </div>
                    </div>
                </form>
            </div>
        </div>

    }
</div>

